{
  "name": "koshi-vox",
  "version": "1.2.5",
  "description": "Voice-To-Text recorder with sound notifications - optimized for macOS",
  "bin": {
    "koshi-vox": "./bin/koshi-vox",
    "vox": "./bin/vox",
    "vox-debug": "./bin/vox-debug",
    "vox-cleanup": "./bin/vox-cleanup",
    "setup-vox": "./scripts/setup-vox-alias",
    "uninstall-vox": "./scripts/uninstall_koshi-vox",
    "vox-install": "./scripts/install.js"
  },
  "scripts": {
    "install": "chmod +x bin/koshi-vox bin/vox bin/vox-debug bin/vox-cleanup scripts/setup-vox-alias scripts/uninstall_koshi-vox scripts/install.js 2>/dev/null || true",
    "preuninstall": "echo \"\\n🗑️  Running vox uninstaller...\" && scripts/uninstall_koshi-vox 2>/dev/null || echo \"Manual cleanup: remove vox aliases from .zshrc\"",
    "koshi-install": "echo '🎤 Installing vox component to koshi-code...'",
    "deps": "node scripts/install.js"
  },
  "keywords": [
    "voice",
    "transcription",
    "whisper",
    "claude-code",
    "koshi-code",
    "macos",
    "m1-4",
    "koshi-vox",
    "clipboard",
    "speech-to-text",
    "optional-component"
  ],
  "author": "KoshiMazaki",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=7.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "peerDependencies": {
    "python3": "*"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/koshi-code/scripts.git"
  },
  "files": [
    "bin/",
    "scripts/",
    "lib/",
    "sounds/",
    "README.md"
  ],
  "preferGlobal": true,
  "koshi-code": {
    "component": "voice-to-text",
    "optional": true,
    "category": "productivity",
    "dependencies": [
      "python3",
      "faster-whisper",
      "soundfile",
      "sox"
    ]
  },
  "packageManager": "npm@>=8.0.0",
  "publishConfig": {
    "access": "public"
  }
}
